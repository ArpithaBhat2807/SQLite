<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginsqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Loginsqlite">
        <activity android:name=".Splash"></activity>
        <activity android:name=".Viewusers" />
        <activity android:name=".DatabaseHelper" />
        <activity android:name=".Deleteuser" />
        <activity android:name=".Updateuser" />
        <activity android:name=".Insertuser" />
        <activity android:name=".Useroperations" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>